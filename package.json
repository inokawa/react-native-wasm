{
  "name": "react-native-wasm",
  "version": "0.1.3",
  "description": "A polyfill to use WebAssembly in React Native.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-wasm.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build": "bob build",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "prepublishOnly": "git clean -d -x -f ./android && git clean -d -x -f ./ios"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "jest": "^26.0.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "^0.18.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react-native": ">=0.59.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/react-native-wasm.git"
  },
  "keywords": [
    "react",
    "react-native",
    "webassembly",
    "wasm",
    "polyfill",
    "ios",
    "android"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/react-native-wasm/issues"
  },
  "homepage": "https://github.com/inokawa/react-native-wasm#readme",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
